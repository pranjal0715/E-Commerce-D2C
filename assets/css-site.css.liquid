}
.newsletter-popup .newsletter-form__submit:hover {
  background: rgba(0, 0, 0, 0.75);
}
.newsletter-popup .newsletter-popup__content-inner {
  width: 100%;
  padding: 25px 10px;
  text-align: center;
}
.newsletter-popup__close {
  position: absolute;
  top: 2px;
  right: 2px;
